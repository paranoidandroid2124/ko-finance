{
  "id": null,
  "uid": "ingest-reliability",
  "title": "Ingest Reliability",
  "tags": [
    "ingest",
    "reliability"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Seed Success Rate (5m)",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(ingest_pipeline_result_total{stage=\"seed.package\",result=~\"success|fallback_success\"}[5m])) / clamp_min(sum(rate(ingest_pipeline_result_total{stage=\"seed.package\"}[5m])), 1e-9)"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Time Since Last Error (seed.package)",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "time() - ingest_pipeline_last_error_timestamp{stage=\"seed.package\"}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Errors by Source (15m rate)",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (source) (rate(ingest_pipeline_errors_total{stage=\"seed.package\"}[15m]))"
        }
      ]
    },
    {
      "id": 4,
      "type": "table",
      "title": "Top Exceptions (seed.package)",
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "auto" }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(5, sum(rate(ingest_pipeline_errors_total{stage=\"seed.package\"}[15m])) by (exception))",
          "format": "table",
          "instant": true
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Process Filing p95 Latency",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 18 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": false
        },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(ingest_pipeline_latency_seconds_bucket{stage=\"process.filing\"}[5m])) by (le))"
        }
      ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "DLQ Pending Entries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "ingest_dlq_entries{status=\"pending\"}"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
