{
  "id": null,
  "uid": "event-export-health",
  "title": "Event Study Export Health",
  "tags": [
    "reports",
    "event-study"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "1m",
  "time": {
    "from": "now-12h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Deleted bundles (15m)",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(event_bundle_cleanup_total{result=\"deleted\"}[15m])"
        }
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Cleanup failures (15m)",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(event_bundle_cleanup_total{result=\"failed\"}[15m])"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Bundle directories",
      "gridPos": { "h": 10, "w": 10, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": false
        },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "event_bundle_directories"
        }
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Retention window (days)",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "d",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "event_bundle_retention_days"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
