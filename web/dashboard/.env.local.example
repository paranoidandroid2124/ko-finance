# Base URL of FastAPI when proxying from Next.js
API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Auth.js secrets
AUTH_SECRET=replace-with-random-string
AUTH_TRUST_HOST=true
NEXTAUTH_URL=http://localhost:3000

# Database (choose either DATABASE_URL or individual fields)
DATABASE_URL=postgres://USER:PASSWORD@HOST:5432/DBNAME
# DB_USER=user
# DB_PASSWORD=password
# DB_HOST=127.0.0.1
# DB_NAME=database
# DB_PORT=5432

# OAuth provider credentials
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=
AUTH_KAKAO_ID=
AUTH_KAKAO_SECRET=
AUTH_NAVER_ID=
AUTH_NAVER_SECRET=
